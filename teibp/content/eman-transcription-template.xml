        <rendition xml:id="hang" n="teibp:hang" scheme="css">
          display:block;
          padding-left:2.5em;
          text-indent:-2.5em;
        </rendition>
        
        <!-- Indentation for verse lines. Results in a “hanging” indent if
					the line wraps. -->
        <rendition xml:id="l-indent-01" n="tiebp:l-indent-01" scheme="css">
